#ifndef GRAPH_H
#define GRAPH_H
#include<iostream>
#include<vector>
class Graph{
public:
Graph(const int&);


private:
struct node{
int n;
node* next;
};
int v;
node* adjList;



};


Graph::Graph(const int& v)
{
this->v=v;
adjList= node[v];
for(int i=0; i!=v; ++i)
{
adjList[i].n=i;
adjList[i].next=NULL;
}

}









#endif
